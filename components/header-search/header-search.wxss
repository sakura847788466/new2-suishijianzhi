@import '/app.wxss';
.head_sxk {
  padding:20rpx;
  border-bottom: 2rpx solid #f2f2f2;
  background:#fff;
}
.awwor {
  width: 24rpx;
  height: 17rpx;
  margin: 0 20rpx;
}
.input-box{
  position:relative;
  flex:1;
}
.input-box input,.input-box .input {
  flex: 1;
  height: 66rpx;
  line-height:66rpx;
  padding-left: 70rpx;
  border-radius: 40rpx;
  background-color: #f2f2f2;
}
.input-box image {
  position: absolute;
  width: 40rpx;
  height: 40rpx;
  top: 50%;
  left: 20rpx;
  margin-top: -20rpx;
}